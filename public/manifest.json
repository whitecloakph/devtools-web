{
  "name": "Kartero",
  "version": "0.0.0",
  "description": "REST client for connecting to your mock and real API endpoints.",
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "manifest_version": 2,
  "permissions": [
    "activeTab",
    "*://*/*",
    "storage"
  ],
  "minimum_chrome_version": "10.0",
  "devtools_page": "devtools.html"
}
